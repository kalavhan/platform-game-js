{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "import/no-unresolved": "off",
      "no-undef": 0,
      "new-cap": 0,
      "class-methods-use-this": 0,
      "func-names": 0,
      "prefer-arrow-callback": 0,
      "prefer-destructuring": 0,
      "object-shorthand": 0,
      "no-unused-expressions": 0  
    }
  }
